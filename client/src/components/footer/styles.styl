@import './../../styl/variables.styl';

.footer {
    text-align: center;
    padding: 2rem 5rem;
    line-height: 1.25rem;
    background: #000639;
}
.footer-links {
    margin-bottom: 1rem;  
    font-weight: 700;
    text-decoration: none;
    color: #fff;  
}

.footer-links > a {
    font-family: "Segoe UI", sans-serif;
    font-weight: 700;
    text-decoration: none;
    color: #fff;
}

.footer-disclaimer {
    font-size: 12px;  
    color: #fff;  
}